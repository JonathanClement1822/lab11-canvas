<!DOCTYPE html>
<html>
<body onload="draw()">
<canvas id="sketch" width="300" height= "500">
</canvas>
<script>
function draw()
 {
	var dog = document.getElementById ('sketch');
	var cat = sketch.getContext ("2d");
	context.fillStyle = "#87ceeb";
	context.fillRect(30,30,50,50);
	context.fillStyle = "#00bfff";
	context.beginPath()
	context.arc(150,50,30,0,6 * Math.PI,true);
	context.closePath();
	context.fill();
}
</script>
</body>
</html>
